@charset "utf-8";

* { margin: 0px; padding: 0px; outline: none; }

body { background: rgb(38, 38, 38); height: 100%; }

#ad_iframe { box-shadow: rgba(0, 0, 0, 0.2) 0px 0px 5px 0px; display: block; left: auto; margin: 0px auto; position: relative; top: auto; }

html { height: 100%; }

.toprow { width: 100%; font-family: Roboto, arial, sans-serif; display: table; height: 30px; font-size: 18px; background-color: rgb(66, 66, 66); }

.btn { display: table; transition: opacity 1s ease 0s, background 0.75s ease 0s; height: 30px; padding-top: 15px; padding-bottom: 15px; background: transparent; padding-right: 0.25em; color: rgb(255, 255, 255); }

#dismiss-button { padding-left: 0.5em; padding-right: 0.5em; }

.btn > div { display: table-cell; vertical-align: middle; padding: 0px 0.25em; }

.skip { opacity: 0.95; float: left; }

.skip svg { height: 1.3em; width: 1.3em; margin-left: -0.3em; margin-right: -0.3em; vertical-align: middle; padding-bottom: 1px; }

.report { opacity: 0.55; float: right; visibility: visible; }

.report img { height: 1.06em; width: 1.06em; }

#report-button { top: 0px; border: 0px; }

.report img { height: 1.5em; width: 1.5em; }

.heading { margin: auto; }

.creative { margin-top: 107px; transition: opacity 1s ease 0s; position: relative; }

#ad_position_box { position: relative; height: 100%; }
